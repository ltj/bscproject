svg
	font 10px sans-serif

.line
	fill none
	stroke #000
	stroke-width 1.5px

.maxline
	stroke #f00
	stroke-width 1.5px
     
.axis path, .axis line
	fill none
	stroke #000
	shape-rendering crispEdges

.progress-meter .background
	fill #ccc

.progress-meter .foreground
	fill #000

.progress-meter text
	font-family "Helvetica Neue", Helvetica, Arial, sans-serif
	font-size 24px
	font-weight bold